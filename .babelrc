{
  "presets": ["@babel/preset-react", "@babel/typescript", ["@babel/preset-env", { "modules": false }]],
  
  "env": {
    "development": {
      "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}